export DOTFILES_DIR="$HOME/Cystem/dotfiles"
export PATH="$DOTFILES_DIR/bin:$PATH";

# 依次导入相关设置
# 1. 在.path,比如切换unix 命令为 gnu 命令, 引入 NVM , GVM , PYENV, Source
# 2. 在.exports 定义环境变量
# 3. 在.functions 定义helper函数和cmd函数
# 4. 在.alias 定义命令
# 5  在.xxx.local 中定义不需要提交的命令
# Load the shell dotfiles, and then some:
for file in "$DOTFILES_DIR/".{path,path.local,exports,exports.local,aliases,aliases.local,functions,functions.local}; do
	[ -r "$file" ] && [ -f "$file" ] && source "$file";
done;
unset file;

